<%- include('partials/header') -%>
<div class="chat">
  <h2>Chat</h2>
  <h3>Hello <%= email %></h3>
  <div class="chat__box">
    <div class="chat__sidebar">
      <% rooms.forEach(({ name }, index) => { %>
      <div class="chat__room">
        <p id="room-<%= index %>"><%= name %></p>
      </div>
      <% }) %>
    </div>
    <div class="chat__messages">
    </div>
  </div>
</div>
<%- include('partials/footer') -%>
